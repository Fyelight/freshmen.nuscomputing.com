  body(class=bodyClass)
    nav.global
      a.logo(href=baseUrl)
        h1
          span.word.word-f F
            span.letters reshmen 
          span.word.word-o O
            span.letters rientation 
          span.word.word-p P
            span.letters rojects 
          span.year 2016
      
      button.btn.menu-btn
        span.glyphicon.glyphicon-menu-hamburger(aria-hidden='true')
        span.sr-only Open Menu
        
      .full-only
        ul.nav
          li: a.btn(href=url('fsc'))
            abbr(title='Freshmen Social Camp') FSC
          li: a.btn(href=url('fow'))
            abbr(title='Freshmen Orientation Week') FOW
          li: a.btn(href=url('rag-flag'))
            span
              | RAG 
              br
              | & FLAG
          li: a.btn(href=url('bash'))
            span BASH
          li: a.btn.btn-primary(href='https://mysurvey.nus.edu.sg/EFM/se.ashx?s=54CCD14B2078EA2B',target='_blank')
            span Join Our Mailing List
      
    //
      ul
        li: a(href=baseUrl + 'about') About
        li: a(href=baseUrl + 'contact') Contact Us

    block header
    
    block main
      
    footer.global
      div.bottom-call
